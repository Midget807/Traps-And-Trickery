{
	"schemaVersion": 1,
	"id": "trapsntrickery",
	"version": "${version}",
	"name": "Traps and Trickery",
	"description": "A little bit of tomfoolery...",
	"authors": [
		"Midget807"
	],
	"contact": {
		"homepage": "https://github.com/Midget807/Traps-And-Trickery",
		"sources": "https://github.com/Midget807/Traps-And-Trickery/branches",
		"issues": "https://github.com/Midget807/Traps-And-Trickery/issues"
	},
	"license": "MIT",
	"icon": "assets/trapsntrickery/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "net.midget807.trapsntrickery.TrapsAndTrickeryMain"
		],
		"client": [
			"net.midget807.trapsntrickery.TrapsAndTrickeryClient"
		],
		"fabric-datagen": [
			"net.midget807.trapsntrickery.TrapsAndTrickeryDataGenerator"
		]
	},
	"mixins": [
		"trapsntrickery.mixins.json",
		{
			"config": "trapsntrickery.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.5",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}